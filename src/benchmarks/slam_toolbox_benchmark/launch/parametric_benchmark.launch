<launch>
  <arg name="main_node_prefix" default="$(optenv LAMBKIN_MAIN_NODE_PREFIX)"/>
  <node pkg="slam_toolbox" type="async_slam_toolbox_node" name="slam_toolbox" required="true"
        launch-prefix="$(arg main_node_prefix)" output="log">
    <rosparam command="load" file="$(find slam_toolbox_benchmark)/config/tum_benchmark.yaml" />
    <rosparam param="resolution" subst_value="True">$(env TEST_PARAM_RESOLUTION)</rosparam>
    <rosparam param="loop_search_space_resolution" subst_value="True">$(env TEST_PARAM_SEARCH_RESOLUTION)</rosparam>
  </node>
</launch>
